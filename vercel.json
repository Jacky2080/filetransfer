{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/test", "destination": "/api/test.js" },
    { "source": "/files", "destination": "/api/fileList.js" },
    { "source": "/file", "destination": "/api/upload.js" },
    { "source": "/download", "destination": "/api/download.js" },
    { "source": "/deepseek/message", "destination": "/api/deepseek.js" },
    { "source": "/success", "destination": "/success/index.html" },
    { "source": "/fail", "destination": "/fail/index.html" },
    { "source": "/deepseek", "destination": "/deepseek/index.html" }
  ],
  "crons": [
    {
      "path": "/api/cleanup.js",
      "schedule": "0 22 * * *"
    }
  ]
}
